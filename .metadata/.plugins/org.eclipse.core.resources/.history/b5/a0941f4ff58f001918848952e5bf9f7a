		getContentPane().add(lblCdigoDeInternamiento);
		
		txtcodi = new JTextField();
		txtcodi.setBounds(154, 8, 86, 20);
		getContentPane().add(txtcodi);
		txtcodi.setColumns(10);
		
		lblCama = new JLabel("Cama:");
		lblCama.setBounds(10, 36, 134, 23);
		getContentPane().add(lblCama);
		
		comboBox = new JComboBox();
		comboBox.setBounds(154, 39, 86, 20);
		getContentPane().add(comboBox);
		
		lblGenerarCodigoCorrelativo = new JLabel("GENERAR CODIGO CORRELATIVO");
		lblGenerarCodigoCorrelativo.setBounds(250, 11, 174, 14);
		getContentPane().add(lblGenerarCodigoCorrelativo);
		
		lblLlamarALas = new JLabel("Llamar a las camas libres del cbobox");
		lblLlamarALas.setBounds(250, 40, 174, 14);
		getContentPane().add(lblLlamarALas);
		
		lblFechaDeIngreso = new JLabel("Fecha de Ingreso:");
		lblFechaDeIngreso.setBounds(10, 70, 134, 14);
		getContentPane().add(lblFechaDeIngreso);
		
		comboBox_1 = new JComboBox();
